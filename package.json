{
  "name": "@ngx-markdown/core",
  "version": "0.1.0-beta.4",
  "description": "Angular 2+ markdown to html using marked a markdown parser and compiler.",
  "author": "Ścibor Rudnicki <scibor.rudnicki@wwwdev.io> (http://ngx-markdown.wwwwdev.io)",
  "homepage": "https://github.com/ngx-markdown/core/blob/master/README.md",
  "main": "dist/bundle.umd.js",
  "module": "dist/index.js",
  "jsnext:main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "contributors": [
    "Ścibor Rudnicki <scibor.rudnicki@wwwdev.io>"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "bundle": "rollup -c rollup.config.js",
    "prebuild": "",
    "build": "./node_modules/.bin/ngc -p tsconfig.json",
    "postbuild": "npm run html && npm run bundle",
    "prepublish": "npm run build",
    "start": "npm run build",
    "html": "cd src && copyfiles **/*.html *.html *.scss **/*.scss ../dist",
    "test": "./node_modules/karma/bin/karma start karma.conf.js"
  },
  "license": "MIT",
  "dependencies": {
    "@types/marked": "0.0.28",
    "lodash-es": "^4.17.4",
    "marked": "^0.3.6"
  },
  "devDependencies": {
    "@angular/common": "^4.3.3",
    "@angular/compiler": "^4.3.3",
    "@angular/compiler-cli": "^4.3.3",
    "@angular/core": "^4.3.3",
    "@angular/platform-browser": "^4.3.3",
    "@angular/platform-browser-dynamic": "^4.3.3",
    "@types/jasmine": "^2.5.53",
    "@types/karma": "^0.13.36",
    "@types/lodash": "^4.14.71",
    "@types/lodash-es": "^4.14.5",
    "@types/node": "^8.0.19",
    "codelyzer": "^3.1.2",
    "copyfiles": "^1.2.0",
    "core-js": "^2.4.1",
    "jasmine": "^2.7.0",
    "jasmine-core": "^2.7.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-rollup-preprocessor": "^4.0.2",
    "karma-typescript": "^3.0.5",
    "lodash": "^4.17.4",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.1",
    "rollup": "^0.45.2",
    "rollup-plugin-angular": "^0.4.4",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-html": "^0.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.4.2",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2",
    "uglify-es": "^3.0.27",
    "zone.js": "^0.8.16"
  },
  "keywords": [
    "Angular"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngx-markdown/core.git"
  },
  "bugs": {
    "url": "https://github.com/ngx-markdown/core/issues"
  }
}
